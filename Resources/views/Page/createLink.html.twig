{% extends 'KitpagesCmsBundle:Page:cms-edit-layout.html.twig' %}

{% block kitpages_cms_body %}
<div class="kit-cms-edit kit-cms">


<h1>Create Page Link</h1>

<form class="kit-cms-form" action="{{ path('kitpages_cms_page_create_link', { 'kitpages_target': kitpages_target } ) }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}

    <input type="submit"/>
</form>

</div>
{% endblock kitpages_cms_body %}